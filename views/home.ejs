<%- include("partials/header"); -%>
<div class="firstpost">
  <h1>Home</h1>
  <p> <%= StartingContent %> </p>
</div>

<div class="postposted">

  <% posts.forEach(post => { %>
  <h2> <%=post.title%></h2>
  <p>
    <%=post.content.substring(0, 100) + " ..." %>
    <a href="/posts/<%=post.title%>">Read More</a>
  </p>

  <%}); %>

</div>



<%- include("partials/footer"); -%>
