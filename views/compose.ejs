<%- include("partials/header"); -%>
<h1>Compose</h1>
<form class="" action="compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle" autocomplete="off">
  </div>

  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" name="postBody" rows="13" cols="100"></textarea>
  </div>

<button class="btn btn-info btn-lg active" type="submit" name="button">Publish</button>

</form>


<%- include("partials/footer"); -%>
